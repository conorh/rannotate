<% @libraries.each do |name,ver_array| %>
<%= name %><br/>
<% ver_array.each do |ver| %>
<% verstring = ver.major.to_s + "." + ver.minor.to_s + "." + ver.release.to_s %>
* <%= link_to verstring, {:action => 'list', :library => name, :version => verstring} %><br/>
<% end %>
<br/><br/>
<% end %>