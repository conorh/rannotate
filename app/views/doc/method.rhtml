<b><%= @method.name %></b><%= @method.parameters %> <% if(@version) then %>Version <%= @version %><% end %>
<div class="description">
<p><%= markup(@method.ra_comment.comment) %></p>
</div>
  
<div class="sourcecode">
<%= render :partial => 'doc/partials/source_code' %>
</div>

<%= render_notes(@container_name, 'method_' + @method.name, '#') %>