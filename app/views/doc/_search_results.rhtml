[<%= pluralize(@results_count, "hit") %>]
<% # get results for each type in order 
[RaModule, RaClass, RaConstant, RaMethod, RaAttribute].each do |type|
  object_array = @search_results[type]
  unless object_array.nil? then
  	%>
		<h4><%=pluralize(object_array.length, type.type_string.capitalize)%></h4>
		<% object_array.each do |object| %>
			<%= link_to_doc(object) %>
		<% 
		end
	end		
end 
%>